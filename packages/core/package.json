{
  "name": "@micro-iframe/core",
  "version": "0.1.0",
  "description": "微前端框架核心库",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "micro-frontend",
    "iframe",
    "core"
  ],
  "license": "MIT",
  "dependencies": {
    "@micro-iframe/types": "workspace:*",
    "@micro-iframe/utils": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.0.0"
  }
}

