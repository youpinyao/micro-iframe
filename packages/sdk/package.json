{
  "name": "@micro-iframe/sdk",
  "version": "0.1.0",
  "description": "微前端子应用 SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "micro-frontend",
    "iframe",
    "sdk"
  ],
  "license": "MIT",
  "dependencies": {
    "@micro-iframe/types": "workspace:*",
    "@micro-iframe/utils": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.4.0",
    "vue-router": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.0.0",
    "vue": "^3.4.0",
    "vue-router": "^4.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  }
}
